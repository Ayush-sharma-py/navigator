<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>grid_divider</name>
  <version>1.0.0</version>
  <description>Nova's localization service, which refines GPS estimates using PCD matching, IMU, and more.</description>
  <maintainer email="Will.Heitman@utdallas.edu">Will Heitman</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <depend>rclcpp</depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>PDAL</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>PCL</build_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
